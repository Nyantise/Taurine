{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:write-all",
    "fs:read-all",
    "fs:allow-read-text-file",
    "fs:allow-resource-read-recursive",
    "fs:allow-app-meta-recursive",
    "fs:allow-appdata-meta-recursive"
  ]
}